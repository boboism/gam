<h1>Listing depreciation_methods</h1>

<table>
  <tr>
    <th>Description</th>
    <th>Index</th>
    <th>Key</th>
    <th>Status</th>
    <th>Value</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @depreciation_methods.each do |depreciation_method| %>
  <tr>
    <td><%= depreciation_method.description %></td>
    <td><%= depreciation_method.index %></td>
    <td><%= depreciation_method.key %></td>
    <td><%= depreciation_method.status %></td>
    <td><%= depreciation_method.value %></td>
    <td><%= link_to 'Show', depreciation_method %></td>
    <td><%= link_to 'Edit', edit_depreciation_method_path(depreciation_method) %></td>
    <td><%= link_to 'Destroy', depreciation_method, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

